<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="RegressionSuite">
		 	<listeners>
		<listener class-name="com.myntrajio.generic.listenerlibrary.Listenerimplementation"></listener>
	</listeners>
	<test thread-count="5" name="chromeTest">
		<parameter name="browser" value="chromeTest"></parameter>
		<classes>
			<class name="com.myntrajio.MensProductTest.MensProductTest" />
			<class name="com.myntrajio.WomensProductTest.WomensProductTest" />
		</classes>
	</test>
	<test thread-count="5" name="FirefoxTest">
		<parameter name="browser" value="FirefoxTest"></parameter>
		<classes>
			<class name="com.myntrajio.KidsProductTest.KidsProductTest" />
		</classes>
	</test>
	<test thread-count="5" name="EdgeTest">
		<parameter name="browser" value="edgeTest"></parameter>
		<classes>
			<class
				name="com.myntrajio.ElectronicProductTest.ElectronicProductTest" />
		</classes>
	</test>


	<!-- ChromeTest -->
</suite> <!-- RegressionSuite -->
